{
  "app": {
    "bundleName": "com.huawei.video",
    "vendor": "huawei",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.huawei.video",
    "name": ".MyApplication",
    "mainAbility": ".MainAbility",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "visible": true,
        "name": ".MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "formsEnabled": false,
        "label": "$string:VideoDemo",
        "type": "page",
        "launchType": "standard",
        "srcPath": "mainAbility",
        "srcLanguage": "js"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index",
          "pages/videopage/videopage"
        ],
        "name": "mainAbility",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": true
        }
      }
    ],
    "distroFilter": {
      "apiVersion": {
        "policy": "include",
        "value": []
      }
    }
  }
}